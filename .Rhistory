h1_hist <- hist(h1,
# breaks = 20,
col = "lightblue",
main = "Histogram of Happiness Score (Frequency) for high-productivity with Normal Curve",
xlab = "Happiness Score",
ylab = "Frequency")
x <- seq(min(h, na.rm = TRUE), max(h1, na.rm = TRUE), length = 200)
curve_y <- dnorm(x,
mean = mean(h1, na.rm = TRUE),
sd = sd(h1, na.rm = TRUE))
curve_y_scaled <- curve_y * max(h1_hist$counts) / max(curve_y)
lines(x, curve_y_scaled, col = "red", lwd = 2)
